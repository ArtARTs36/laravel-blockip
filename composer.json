{
    "name": "artarts36/laravel-block-ip",
    "description": "Laravel Block IP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "aukrainskiy",
            "email": "aukrainskiy@phoenixit.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "laravel/framework": "6.* || 5.* || 7.* || 8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.* | 8.*",
        "squizlabs/php_codesniffer": "3.*",
        "doctrine/dbal": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ArtARTs36\\LaravelBlockIp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtARTs36\\LaravelBlockIp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -v --coverage-text --colors=never --configuration phpunit.xml --stop-on-failure --stop-on-risky",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "lint": [
            "./vendor/bin/phpcs --standard=PSR2 src/"
        ]
    }
}
